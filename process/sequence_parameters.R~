cc <- as.numeric(row.names(nasa_data[which(nasa_data$subject_1 == "DLR_111" & nasa_data$Time == "t12" & nasa_data$imageScan_ID == 22),]))
# 852: wo_2015, DLR_001, t12, Control, CGN (imageScan_ID = 23... duplicate scan)
dd <- as.numeric(row.names(nasa_data[which(nasa_data$subject_1 == "DLR_001" & nasa_data$Time == "t12" & nasa_data$imageScan_ID == 23),]))




nasa_data <- nasa_data[-c(a, b, c, d, e, f, g, h, i, j, k, l, m, n, o, p, q, r, s, t, u, v, w, q, x, y, z, aa, bb, cc, dd),]


# Make Protocol into factor
nasa_data$qluxName <- factor(nasa_data$qluxName)

# change dx and dy to 4 digits and Te to 3
nasa_data$dx <- round(nasa_data$dx,digits=4)
nasa_data$dy <- round(nasa_data$dy,digits=4)
nasa_data$dz <- round(nasa_data$dz,digits=2)
nasa_data$Te <- round(nasa_data$Te,digits=3)

# -------------------------------- T1 -------------------------------- #
T1_data <- nasa_data[nasa_data$qluxName == "3D_BRAVO_.9x.9x1mm" | nasa_data$qluxName == "MPRAGE_TI1100" | nasa_data$qluxName == "MPRAGE_TI1100_p2" | nasa_data$qluxName == "SC_3D_BRAVO_0.8_ISOTROPIC",]
T1_data <- rbind(T1_data, nasa_data[nasa_data$subject_1 == "concordia_105" & nasa_data$Time == "t12" & nasa_data$qluxName == "SC_3D_BRAVO_.9x.9x1mm",])
# Filter for bblid, scanid, qluxName, dx, dy, dz, FlipAngle, AcquisitionMatrix, PhaseDirection, Te, Tr
T1_data <- T1_data[,c("bblid", "scanid", "winterover", "subject_1", "Time", "scanner", "group", "qluxName", "dx", "dy", "dz", "FlipAngle", "AcquisitionMatrix", "PhaseDirection", "Te", "Tr")]
write.csv(T1_data, file="/home/ebutler/erb_data/nasa/nasa_T1_acparams.csv", row.names=F)

# Set 1 (N = 103)
T1_set1 <- T1_data[T1_data$dx == 0.9375 & T1_data$dy == 0.9375 & T1_data$dz == 1 & T1_data$FlipAngle == 9 & T1_data$PhaseDirection == "ROW" & T1_data$Te == 3.51 & T1_data$Tr == 1810,]

