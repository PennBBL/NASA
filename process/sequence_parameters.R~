T2_data <- T2_data[,c("bblid", "scanid", "winterover", "subject_1", "Time", "scanner", "group", "qluxName", "dx", "dy", "dz", "FlipAngle", "AcquisitionMatrix", "PhaseDirection", "Te", "Tr")]

# Scan number naming issue for MB t3... info entered manually (DO THIS)... overcame by downloading mid-protocol maker
df <- data.frame(matrix(NA, nrow = 1, ncol = 17))
colnames(df) <- c("bblid", "scanid", "winterover", "subject_1", "Time", "scanner", "group", "nVolumes", "qluxName", "dx", "dy", "dz", "FlipAngle", "AcquisitionMatrix", "PhaseDirection", "Te", "Tr")
df[1,] <- c("Phantom_MB","Phantom_MB_T3","phantoms","BM","t3","HOB","Phantom","HIGH_RES_HIPP_PD",0.293,0.293,2,90,"0\384\384\0","ROW",42.888,8020)
T2_data <- rbind(T2_data, df)

T2_data$nVolumes <- round(as.numeric(T2_data$nVolumes), digits = 0)
T2_data$dx <- round(as.numeric(T2_data$dx), digits = 4)
T2_data$dy <- round(as.numeric(T2_data$dy), digits = 4)
T2_data$dz <- round(as.numeric(T2_data$dz), digits = 2)
T2_data$Te <- round(as.numeric(T2_data$Te), digits = 0)
T2_data$Tr <- round(as.numeric(T2_data$Tr), digits = 0)


write.csv(T2_data, file="/home/ebutler/erb_data/nasa/nasa_T2_acparams.csv", row.names=F)

