set1_num_phantomst1 <- nrow(SVOLTrecall_set1[SVOLTrecall_set1$winterover == "phantoms" & SVOLTrecall_set1$Time == "t1",])
set1_num_phantomst2 <- nrow(SVOLTrecall_set1[SVOLTrecall_set1$winterover == "phantoms" & SVOLTrecall_set1$Time == "t2",])
set1_num_phantomst3 <- nrow(SVOLTrecall_set1[SVOLTrecall_set1$winterover == "phantoms" & SVOLTrecall_set1$Time == "t3",])
set1_num_phantomst4 <- nrow(SVOLTrecall_set1[SVOLTrecall_set1$winterover == "phantoms" & SVOLTrecall_set1$Time == "t4",])

