### This script summarizes the sequence parameters for T1's, T2's and DTI
### 
### Ellyn Butler
### December 12, 2018

# Source my functions
source("/home/ebutler/scripts/ButlerPlotFuncs/plotFuncs.R")

# Load my data
nasa_data <- read.csv("/home/ebutler/erb_data/nasa/butellyn_12_21_2018_11_0_2.csv", header=T)

# Rename identification columns
names(nasa_data)[names(nasa_data) == "MR.ID"] <- "scanid"
names(nasa_data)[names(nasa_data) == "Subject"] <- "bblid"

# Put in usual identifiers
nasa_data <- bblidscan_NASAAntartica(nasa_data)
nasa_data <- scanningSite_NASAAntartica(nasa_data)

#### Take out duplicates
# Take out row 1557 (hippocampus that we didn't use for PK), 466 double for DTI
# 183: wo_2015, concordia_006, t12, Crew, CHR (imageScan_ID = 10... duplicate scan)
a <- as.numeric(row.names(nasa_data[which(nasa_data$subject_1 == "concordia_006" & nasa_data$Time == "t12" & nasa_data$imageScan_ID == 10),]))
# 466: wo_2016, concordia_101, t12, Crew, CHR (imageScan_ID = 16... duplicate scan)
b <- as.numeric(row.names(nasa_data[which(nasa_data$subject_1 == "concordia_101" & nasa_data$Time == "t12" & nasa_data$imageScan_ID == 16),]))
