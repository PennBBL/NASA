		}
	}
	Limbicsummary_MeanSD <- summarySE(Limbic_CGN_CHR_CGN_MeanSDTrans, measurevar="Limbic_GMD_MeanSD", groupvars=c("group", "ScanOrder"))
	Limbic_MeanSD_CHR_plot <- ggplot(Limbicsummary_MeanSD, aes(x=ScanOrder, y=Limbic_GMD_MeanSD, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("Limbic (+/- SE)") +
		ylim(-1.5, 3) +
    		geom_errorbar(aes(ymin=Limbic_GMD_MeanSD-se, ymax=Limbic_GMD_MeanSD+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean SD Trans - Limbic Gray Matter Density - CGN-CHR-CGN")
