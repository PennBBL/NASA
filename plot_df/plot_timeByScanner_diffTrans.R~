	FrontDorssummary$group <- factor(FrontDorssummary$group)
	# (transform)
	FrontDors_CGN_CHR_CGN_MeanTrans <- FrontDors_CGN_CHR_CGN2
	FrontDors_CGN_CHR_CGN_MeanTrans$FrontDors_GMD_Mean <- NA
	for (row in 1:nrow(FrontDors_CGN_CHR_CGN_MeanTrans)) {
		if (FrontDors_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "First") {
			first_mean <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "First", "FrontDors_GMD"]
			FrontDors_CGN_CHR_CGN_MeanTrans[row, "FrontDors_GMD_Mean"] <- (FrontDors_CGN_CHR_CGN_MeanTrans[row, "FrontDors_GMD"] - first_mean)
		} else if (FrontDors_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "Second") {
			second_mean <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "Second", "FrontDors_GMD"]
			FrontDors_CGN_CHR_CGN_MeanTrans[row, "FrontDors_GMD_Mean"] <- (FrontDors_CGN_CHR_CGN_MeanTrans[row, "FrontDors_GMD"] - second_mean)
		} else {
			third_mean <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "Third", "FrontDors_GMD"]
			FrontDors_CGN_CHR_CGN_MeanTrans[row, "FrontDors_GMD_Mean"] <- (FrontDors_CGN_CHR_CGN_MeanTrans[row, "FrontDors_GMD"] - third_mean)
		}
	}
	FrontDorssummary_Mean <- summarySE(FrontDors_CGN_CHR_CGN_MeanTrans, measurevar="FrontDors_GMD_Mean", groupvars=c("group", "ScanOrder"))
	FrontDors_Mean_CHR_plot <- ggplot(FrontDorssummary_Mean, aes(x=ScanOrder, y=FrontDors_GMD_Mean, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("FrontDors (+/- SE)") +
		ylim(-.1, .1) +
    		geom_errorbar(aes(ymin=FrontDors_GMD_Mean-se, ymax=FrontDors_GMD_Mean+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean Trans - FrontDors Gray Matter Density - CGN-CHR-CGN")
	# Temporal_GMD
	Temporal_CGN_CHR_CGN2 <- CGN_CHR_CGN[, c("group", "subject_1", "ScanOrder", "Temporal_GMD")] 
	Temporalsummary <- summarySE(Temporal_CGN_CHR_CGN2, measurevar="Temporal_GMD", groupvars=c("group", "ScanOrder"))
	Temporalsummary$ScanOrder <- factor(Temporalsummary$ScanOrder)
	Temporalsummary$group <- factor(Temporalsummary$group)
	# (transform)
	Temporal_CGN_CHR_CGN_MeanTrans <- Temporal_CGN_CHR_CGN2
	Temporal_CGN_CHR_CGN_MeanTrans$Temporal_GMD_Mean <- NA
	for (row in 1:nrow(Temporal_CGN_CHR_CGN_MeanTrans)) {
		if (Temporal_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "First") {
			first_mean <- Temporalsummary[Temporalsummary$group == "Phantom" & Temporalsummary$ScanOrder == "First", "Temporal_GMD"]
			Temporal_CGN_CHR_CGN_MeanTrans[row, "Temporal_GMD_Mean"] <- (Temporal_CGN_CHR_CGN_MeanTrans[row, "Temporal_GMD"] - first_mean)
		} else if (Temporal_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "Second") {
			second_mean <- Temporalsummary[Temporalsummary$group == "Phantom" & Temporalsummary$ScanOrder == "Second", "Temporal_GMD"]
			Temporal_CGN_CHR_CGN_MeanTrans[row, "Temporal_GMD_Mean"] <- (Temporal_CGN_CHR_CGN_MeanTrans[row, "Temporal_GMD"] - second_mean)
		} else {
			third_mean <- Temporalsummary[Temporalsummary$group == "Phantom" & Temporalsummary$ScanOrder == "Third", "Temporal_GMD"]
			Temporal_CGN_CHR_CGN_MeanTrans[row, "Temporal_GMD_Mean"] <- (Temporal_CGN_CHR_CGN_MeanTrans[row, "Temporal_GMD"] - third_mean)
		}
	}
	Temporalsummary_Mean <- summarySE(Temporal_CGN_CHR_CGN_MeanTrans, measurevar="Temporal_GMD_Mean", groupvars=c("group", "ScanOrder"))
	Temporal_Mean_CHR_plot <- ggplot(Temporalsummary_Mean, aes(x=ScanOrder, y=Temporal_GMD_Mean, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("Temporal (+/- SE)") +
		ylim(-.1, .1) +
    		geom_errorbar(aes(ymin=Temporal_GMD_Mean-se, ymax=Temporal_GMD_Mean+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean Trans - Temporal Gray Matter Density - CGN-CHR-CGN")
	# Parietal_GMD
	Parietal_CGN_CHR_CGN2 <- CGN_CHR_CGN[, c("group", "subject_1", "ScanOrder", "Parietal_GMD")] 
	Parietalsummary <- summarySE(Parietal_CGN_CHR_CGN2, measurevar="Parietal_GMD", groupvars=c("group", "ScanOrder"))
	Parietalsummary$ScanOrder <- factor(Parietalsummary$ScanOrder)
	Parietalsummary$group <- factor(Parietalsummary$group)
	# (transform)
	Parietal_CGN_CHR_CGN_MeanTrans <- Parietal_CGN_CHR_CGN2
	Parietal_CGN_CHR_CGN_MeanTrans$Parietal_GMD_Mean <- NA
	for (row in 1:nrow(Parietal_CGN_CHR_CGN_MeanTrans)) {
		if (Parietal_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "First") {
			first_mean <- Parietalsummary[Parietalsummary$group == "Phantom" & Parietalsummary$ScanOrder == "First", "Parietal_GMD"]
			Parietal_CGN_CHR_CGN_MeanTrans[row, "Parietal_GMD_Mean"] <- (Parietal_CGN_CHR_CGN_MeanTrans[row, "Parietal_GMD"] - first_mean)
		} else if (Parietal_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "Second") {
			second_mean <- Parietalsummary[Parietalsummary$group == "Phantom" & Parietalsummary$ScanOrder == "Second", "Parietal_GMD"]
			Parietal_CGN_CHR_CGN_MeanTrans[row, "Parietal_GMD_Mean"] <- (Parietal_CGN_CHR_CGN_MeanTrans[row, "Parietal_GMD"] - second_mean)
		} else {
			third_mean <- Parietalsummary[Parietalsummary$group == "Phantom" & Parietalsummary$ScanOrder == "Third", "Parietal_GMD"]
			Parietal_CGN_CHR_CGN_MeanTrans[row, "Parietal_GMD_Mean"] <- (Parietal_CGN_CHR_CGN_MeanTrans[row, "Parietal_GMD"] - third_mean)
		}
	}
	Parietalsummary_Mean <- summarySE(Parietal_CGN_CHR_CGN_MeanTrans, measurevar="Parietal_GMD_Mean", groupvars=c("group", "ScanOrder"))
	Parietal_Mean_CHR_plot <- ggplot(Parietalsummary_Mean, aes(x=ScanOrder, y=Parietal_GMD_Mean, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("Parietal (+/- SE)") +
		ylim(-.1, .1) +
    		geom_errorbar(aes(ymin=Parietal_GMD_Mean-se, ymax=Parietal_GMD_Mean+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean Trans - Parietal Gray Matter Density - CGN-CHR-CGN")
	# Occipital_GMD
	Occipital_CGN_CHR_CGN2 <- CGN_CHR_CGN[, c("group", "subject_1", "ScanOrder", "Occipital_GMD")]
	Occipitalsummary <- summarySE(Occipital_CGN_CHR_CGN2, measurevar="Occipital_GMD", groupvars=c("group", "ScanOrder"))
	Occipitalsummary$ScanOrder <- factor(Occipitalsummary$ScanOrder)
	Occipitalsummary$group <- factor(Occipitalsummary$group)
	# (transform)
	Occipital_CGN_CHR_CGN_MeanTrans <- Occipital_CGN_CHR_CGN2
	Occipital_CGN_CHR_CGN_MeanTrans$Occipital_GMD_Mean <- NA
	for (row in 1:nrow(Occipital_CGN_CHR_CGN_MeanTrans)) {
		if (Occipital_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "First") {
			first_mean <- Occipitalsummary[Occipitalsummary$group == "Phantom" & Occipitalsummary$ScanOrder == "First", "Occipital_GMD"]
			Occipital_CGN_CHR_CGN_MeanTrans[row, "Occipital_GMD_Mean"] <- (Occipital_CGN_CHR_CGN_MeanTrans[row, "Occipital_GMD"] - first_mean)
		} else if (Occipital_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "Second") {
			second_mean <- Occipitalsummary[Occipitalsummary$group == "Phantom" & Occipitalsummary$ScanOrder == "Second", "Occipital_GMD"]
			Occipital_CGN_CHR_CGN_MeanTrans[row, "Occipital_GMD_Mean"] <- (Occipital_CGN_CHR_CGN_MeanTrans[row, "Occipital_GMD"] - second_mean)
		} else {
			third_mean <- Occipitalsummary[Occipitalsummary$group == "Phantom" & Occipitalsummary$ScanOrder == "Third", "Occipital_GMD"]
			Occipital_CGN_CHR_CGN_MeanTrans[row, "Occipital_GMD_Mean"] <- (Occipital_CGN_CHR_CGN_MeanTrans[row, "Occipital_GMD"] - third_mean)
		}
	}
	Occipitalsummary_Mean <- summarySE(Occipital_CGN_CHR_CGN_MeanTrans, measurevar="Occipital_GMD_Mean", groupvars=c("group", "ScanOrder"))
	Occipital_Mean_CHR_plot <- ggplot(Occipitalsummary_Mean, aes(x=ScanOrder, y=Occipital_GMD_Mean, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("Occipital (+/- SE)") +
		ylim(-.1, .1) +
    		geom_errorbar(aes(ymin=Occipital_GMD_Mean-se, ymax=Occipital_GMD_Mean+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean Trans - Occipital Gray Matter Density - CGN-CHR-CGN")

	# ---------------------------------------------------------------------------- #

	
	####################### MAKE PLOTS #######################

	# HOB
	pdf(file = "/home/ebutler/nasa_plots/CGNHOBCGN_gmd.pdf", width=18, height=6)
	grid.arrange(AGMD_raw_HOB_plot, AGMD_MeanSD_HOB_plot, AGMD_Mean_HOB_plot, ncol=3)
	grid.arrange(BasGang_raw_HOB_plot, BasGang_MeanSD_HOB_plot, BasGang_Mean_HOB_plot, ncol=3)
	grid.arrange(Limbic_raw_HOB_plot, Limbic_MeanSD_HOB_plot, Limbic_Mean_HOB_plot, ncol=3)
