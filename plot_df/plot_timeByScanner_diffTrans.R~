			first_mean <- ACTsummary[ACTsummary$group == "Phantom" & ACTsummary$ScanOrder == "First", "ACT"]
			ACT_CGN_HOB_CGN_MeanTrans[row, "ACT_Mean"] <- (ACT_CGN_HOB_CGN_MeanTrans[row, "ACT"] - first_mean)
		} else if (ACT_CGN_HOB_CGN_MeanTrans[row, "ScanOrder"] == "Second") {
			second_mean <- ACTsummary[ACTsummary$group == "Phantom" & ACTsummary$ScanOrder == "Second", "ACT"]
			ACT_CGN_HOB_CGN_MeanTrans[row, "ACT_Mean"] <- (ACT_CGN_HOB_CGN_MeanTrans[row, "ACT"] - second_mean)
