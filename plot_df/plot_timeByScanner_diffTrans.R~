	# --------------------------------- MeanSD --------------------------------- #
	### CGN-HOB-CGN
	# AHGMV 
	AHGMV_CGN_HOB_CGN2 <- CGN_HOB_CGN[, c("group", "subject_1", "ScanOrder", "AHGMV")] 
	AHGMVsummary <- summarySE(AHGMV_CGN_HOB_CGN2, measurevar="AHGMV", groupvars=c("group", "ScanOrder"))
	AHGMVsummary$ScanOrder <- factor(AHGMVsummary$ScanOrder)
	AHGMVsummary$group <- factor(AHGMVsummary$group)
	# (transform)
	AHGMV_CGN_HOB_CGN_MeanSDTrans <- AHGMV_CGN_HOB_CGN2
	AHGMV_CGN_HOB_CGN_MeanSDTrans$AHGMV_MeanSD <- NA
	for (row in 1:nrow(AHGMV_CGN_HOB_CGN_MeanSDTrans)) {
		if (AHGMV_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "First") {
			first_mean <- AHGMVsummary[AHGMVsummary$group == "Phantom" & AHGMVsummary$ScanOrder == "First", "AHGMV"]
			first_sd <- AHGMVsummary[AHGMVsummary$group == "Phantom" & AHGMVsummary$ScanOrder == "First", "sd"]
			AHGMV_CGN_HOB_CGN_MeanSDTrans[row, "AHGMV_MeanSD"] <- (AHGMV_CGN_HOB_CGN_MeanSDTrans[row, "AHGMV"] - first_mean)/first_sd
		} else if (AHGMV_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "Second") {
			second_mean <- AHGMVsummary[AHGMVsummary$group == "Phantom" & AHGMVsummary$ScanOrder == "Second", "AHGMV"]
			second_sd <- AHGMVsummary[AHGMVsummary$group == "Phantom" & AHGMVsummary$ScanOrder == "Second", "sd"]
			AHGMV_CGN_HOB_CGN_MeanSDTrans[row, "AHGMV_MeanSD"] <- (AHGMV_CGN_HOB_CGN_MeanSDTrans[row, "AHGMV"] - second_mean)/second_sd
		} else {
			third_mean <- AHGMVsummary[AHGMVsummary$group == "Phantom" & AHGMVsummary$ScanOrder == "Third", "AHGMV"]
			third_sd <- AHGMVsummary[AHGMVsummary$group == "Phantom" & AHGMVsummary$ScanOrder == "Third", "sd"]
			AHGMV_CGN_HOB_CGN_MeanSDTrans[row, "AHGMV_MeanSD"] <- (AHGMV_CGN_HOB_CGN_MeanSDTrans[row, "AHGMV"] - third_mean)/third_sd
		}
	}
	AHGMVsummary_MeanSD <- summarySE(AHGMV_CGN_HOB_CGN_MeanSDTrans, measurevar="AHGMV_MeanSD", groupvars=c("group", "ScanOrder"))
	AHGMV_MeanSD_HOB_plot <- ggplot(AHGMVsummary_MeanSD, aes(x=ScanOrder, y=AHGMV_MeanSD, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("AHGMV (+/- SE)") +
    		geom_errorbar(aes(ymin=AHGMV_MeanSD-se, ymax=AHGMV_MeanSD+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean SD Trans - Gray Matter Volume - CGN-HOB-CGN")
	# AHCGMV
	AHCGMV_CGN_HOB_CGN2 <- CGN_HOB_CGN[, c("group", "subject_1", "ScanOrder", "AHCGMV")] 
