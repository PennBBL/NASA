	# --------------------------------- MeanSD --------------------------------- #
	### CGN-HOB-CGN
	# AGMD
	AGMD_CGN_HOB_CGN2 <- CGN_HOB_CGN[, c("group", "subject_1", "ScanOrder", "AGMD")] 
	AGMDsummary <- summarySE(AGMD_CGN_HOB_CGN2, measurevar="AGMD", groupvars=c("group", "ScanOrder"))
	AGMDsummary$ScanOrder <- factor(AGMDsummary$ScanOrder)
	AGMDsummary$group <- factor(AGMDsummary$group)
	# (transform)
	AGMD_CGN_HOB_CGN_MeanSDTrans <- AGMD_CGN_HOB_CGN2
	AGMD_CGN_HOB_CGN_MeanSDTrans$AGMD_MeanSD <- NA
	for (row in 1:nrow(AGMD_CGN_HOB_CGN_MeanSDTrans)) {
		if (AGMD_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "First") {
			first_mean <- AGMDsummary[AGMDsummary$group == "Phantom" & AGMDsummary$ScanOrder == "First", "AGMD"]
			first_stddev <- AGMDsummary[AGMDsummary$group == "Phantom" & AGMDsummary$ScanOrder == "First", "sd"]
			AGMD_CGN_HOB_CGN_MeanSDTrans[row, "AGMD_MeanSD"] <- (AGMD_CGN_HOB_CGN_MeanSDTrans[row, "AGMD"] - first_mean)/first_stddev
		} else if (AGMD_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "Second") {
			second_mean <- AGMDsummary[AGMDsummary$group == "Phantom" & AGMDsummary$ScanOrder == "Second", "AGMD"]
			second_stddev <- AGMDsummary[AGMDsummary$group == "Phantom" & AGMDsummary$ScanOrder == "Second", "sd"]
			AGMD_CGN_HOB_CGN_MeanSDTrans[row, "AGMD_MeanSD"] <- (AGMD_CGN_HOB_CGN_MeanSDTrans[row, "AGMD"] - second_mean)/second_stddev
		} else {
			third_mean <- AGMDsummary[AGMDsummary$group == "Phantom" & AGMDsummary$ScanOrder == "Third", "AGMD"]
			third_stddev <- AGMDsummary[AGMDsummary$group == "Phantom" & AGMDsummary$ScanOrder == "Third", "sd"]
			AGMD_CGN_HOB_CGN_MeanSDTrans[row, "AGMD_MeanSD"] <- (AGMD_CGN_HOB_CGN_MeanSDTrans[row, "AGMD"] - third_mean)/third_stddev
		}
	}
	AGMDsummary_MeanSD <- summarySE(AGMD_CGN_HOB_CGN_MeanSDTrans, measurevar="AGMD_MeanSD", groupvars=c("group", "ScanOrder"))
	AGMD_MeanSD_HOB_plot <- ggplot(AGMDsummary_MeanSD, aes(x=ScanOrder, y=AGMD_MeanSD, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("AGMD (+/- SE)") +
		ylim(-1.5, 3) +
    		geom_errorbar(aes(ymin=AGMD_MeanSD-se, ymax=AGMD_MeanSD+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean SD Trans - Average Gray Matter Density - CGN-HOB-CGN")
	# BasGang_GMD
	BasGang_CGN_HOB_CGN2 <- CGN_HOB_CGN[, c("group", "subject_1", "ScanOrder", "BasGang_GMD")] 
	BasGangsummary <- summarySE(BasGang_CGN_HOB_CGN2, measurevar="BasGang_GMD", groupvars=c("group", "ScanOrder"))
	BasGangsummary$ScanOrder <- factor(BasGangsummary$ScanOrder)
	BasGangsummary$group <- factor(BasGangsummary$group)
	# (transform)
	BasGang_CGN_HOB_CGN_MeanSDTrans <- BasGang_CGN_HOB_CGN2
	BasGang_CGN_HOB_CGN_MeanSDTrans$BasGang_GMD_MeanSD <- NA
	for (row in 1:nrow(BasGang_CGN_HOB_CGN_MeanSDTrans)) {
		if (BasGang_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "First") {
			first_mean <- BasGangsummary[BasGangsummary$group == "Phantom" & BasGangsummary$ScanOrder == "First", "BasGang_GMD"]
			first_stddev <- BasGangsummary[BasGangsummary$group == "Phantom" & BasGangsummary$ScanOrder == "First", "sd"]
			BasGang_CGN_HOB_CGN_MeanSDTrans[row, "BasGang_GMD_MeanSD"] <- (BasGang_CGN_HOB_CGN_MeanSDTrans[row, "BasGang_GMD"] - first_mean)/first_stddev
		} else if (BasGang_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "Second") {
			second_mean <- BasGangsummary[BasGangsummary$group == "Phantom" & BasGangsummary$ScanOrder == "Second", "BasGang_GMD"]
			second_stddev <- BasGangsummary[BasGangsummary$group == "Phantom" & BasGangsummary$ScanOrder == "Second", "sd"]
			BasGang_CGN_HOB_CGN_MeanSDTrans[row, "BasGang_GMD_MeanSD"] <- (BasGang_CGN_HOB_CGN_MeanSDTrans[row, "BasGang_GMD"] - second_mean)/second_stddev
		} else {
			third_mean <- BasGangsummary[BasGangsummary$group == "Phantom" & BasGangsummary$ScanOrder == "Third", "BasGang_GMD"]
			third_stddev <- BasGangsummary[BasGangsummary$group == "Phantom" & BasGangsummary$ScanOrder == "Third", "sd"]
			BasGang_CGN_HOB_CGN_MeanSDTrans[row, "BasGang_GMD_MeanSD"] <- (BasGang_CGN_HOB_CGN_MeanSDTrans[row, "BasGang_GMD"] - third_mean)/third_stddev
		}
	}
	BasGangsummary_MeanSD <- summarySE(BasGang_CGN_HOB_CGN_MeanSDTrans, measurevar="BasGang_GMD_MeanSD", groupvars=c("group", "ScanOrder"))
	BasGang_MeanSD_HOB_plot <- ggplot(BasGangsummary_MeanSD, aes(x=ScanOrder, y=BasGang_GMD_MeanSD, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("BasGang (+/- SE)") +
		ylim(-1.5, 3) +
    		geom_errorbar(aes(ymin=BasGang_GMD_MeanSD-se, ymax=BasGang_GMD_MeanSD+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean SD Trans - BasGang Gray Matter Density - CGN-HOB-CGN")
	# Limbic_GMD
	Limbic_CGN_HOB_CGN2 <- CGN_HOB_CGN[, c("group", "subject_1", "ScanOrder", "Limbic_GMD")] 
	Limbicsummary <- summarySE(Limbic_CGN_HOB_CGN2, measurevar="Limbic_GMD", groupvars=c("group", "ScanOrder"))
	Limbicsummary$ScanOrder <- factor(Limbicsummary$ScanOrder)
	Limbicsummary$group <- factor(Limbicsummary$group)
	# (transform)
	Limbic_CGN_HOB_CGN_MeanSDTrans <- Limbic_CGN_HOB_CGN2
	Limbic_CGN_HOB_CGN_MeanSDTrans$Limbic_GMD_MeanSD <- NA
	for (row in 1:nrow(Limbic_CGN_HOB_CGN_MeanSDTrans)) {
		if (Limbic_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "First") {
			first_mean <- Limbicsummary[Limbicsummary$group == "Phantom" & Limbicsummary$ScanOrder == "First", "Limbic_GMD"]
			first_stddev <- Limbicsummary[Limbicsummary$group == "Phantom" & Limbicsummary$ScanOrder == "First", "sd"]
			Limbic_CGN_HOB_CGN_MeanSDTrans[row, "Limbic_GMD_MeanSD"] <- (Limbic_CGN_HOB_CGN_MeanSDTrans[row, "Limbic_GMD"] - first_mean)/first_stddev
		} else if (Limbic_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "Second") {
			second_mean <- Limbicsummary[Limbicsummary$group == "Phantom" & Limbicsummary$ScanOrder == "Second", "Limbic_GMD"]
			second_stddev <- Limbicsummary[Limbicsummary$group == "Phantom" & Limbicsummary$ScanOrder == "Second", "sd"]
			Limbic_CGN_HOB_CGN_MeanSDTrans[row, "Limbic_GMD_MeanSD"] <- (Limbic_CGN_HOB_CGN_MeanSDTrans[row, "Limbic_GMD"] - second_mean)/second_stddev
		} else {
			third_mean <- Limbicsummary[Limbicsummary$group == "Phantom" & Limbicsummary$ScanOrder == "Third", "Limbic_GMD"]
			third_stddev <- Limbicsummary[Limbicsummary$group == "Phantom" & Limbicsummary$ScanOrder == "Third", "sd"]
			Limbic_CGN_HOB_CGN_MeanSDTrans[row, "Limbic_GMD_MeanSD"] <- (Limbic_CGN_HOB_CGN_MeanSDTrans[row, "Limbic_GMD"] - third_mean)/third_stddev
		}
	}
	Limbicsummary_MeanSD <- summarySE(Limbic_CGN_HOB_CGN_MeanSDTrans, measurevar="Limbic_GMD_MeanSD", groupvars=c("group", "ScanOrder"))
	Limbic_MeanSD_HOB_plot <- ggplot(Limbicsummary_MeanSD, aes(x=ScanOrder, y=Limbic_GMD_MeanSD, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("Limbic (+/- SE)") +
		ylim(-1.5, 3) +
    		geom_errorbar(aes(ymin=Limbic_GMD_MeanSD-se, ymax=Limbic_GMD_MeanSD+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean SD Trans - Limbic Gray Matter Density - CGN-HOB-CGN")
	# FrontOrb_GMD
	FrontOrb_CGN_HOB_CGN2 <- CGN_HOB_CGN[, c("group", "subject_1", "ScanOrder", "FrontOrb_GMD")] 
	FrontOrbsummary <- summarySE(FrontOrb_CGN_HOB_CGN2, measurevar="FrontOrb_GMD", groupvars=c("group", "ScanOrder"))
	FrontOrbsummary$ScanOrder <- factor(FrontOrbsummary$ScanOrder)
	FrontOrbsummary$group <- factor(FrontOrbsummary$group)
	# (transform)
	FrontOrb_CGN_HOB_CGN_MeanSDTrans <- FrontOrb_CGN_HOB_CGN2
	FrontOrb_CGN_HOB_CGN_MeanSDTrans$FrontOrb_GMD_MeanSD <- NA
	for (row in 1:nrow(FrontOrb_CGN_HOB_CGN_MeanSDTrans)) {
		if (FrontOrb_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "First") {
			first_mean <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "First", "FrontOrb_GMD"]
			first_stddev <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "First", "sd"]
			FrontOrb_CGN_HOB_CGN_MeanSDTrans[row, "FrontOrb_GMD_MeanSD"] <- (FrontOrb_CGN_HOB_CGN_MeanSDTrans[row, "FrontOrb_GMD"] - first_mean)/first_stddev
		} else if (FrontOrb_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "Second") {
			second_mean <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "Second", "FrontOrb_GMD"]
			second_stddev <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "Second", "sd"]
			FrontOrb_CGN_HOB_CGN_MeanSDTrans[row, "FrontOrb_GMD_MeanSD"] <- (FrontOrb_CGN_HOB_CGN_MeanSDTrans[row, "FrontOrb_GMD"] - second_mean)/second_stddev
		} else {
			third_mean <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "Third", "FrontOrb_GMD"]
			third_stddev <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "Third", "sd"]
			FrontOrb_CGN_HOB_CGN_MeanSDTrans[row, "FrontOrb_GMD_MeanSD"] <- (FrontOrb_CGN_HOB_CGN_MeanSDTrans[row, "FrontOrb_GMD"] - third_mean)/third_stddev
		}
	}
	FrontOrbsummary_MeanSD <- summarySE(FrontOrb_CGN_HOB_CGN_MeanSDTrans, measurevar="FrontOrb_GMD_MeanSD", groupvars=c("group", "ScanOrder"))
	FrontOrb_MeanSD_HOB_plot <- ggplot(FrontOrbsummary_MeanSD, aes(x=ScanOrder, y=FrontOrb_GMD_MeanSD, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("FrontOrb (+/- SE)") +
		ylim(-1.5, 3) +
    		geom_errorbar(aes(ymin=FrontOrb_GMD_MeanSD-se, ymax=FrontOrb_GMD_MeanSD+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean SD Trans - FrontOrb Gray Matter Density - CGN-HOB-CGN")
