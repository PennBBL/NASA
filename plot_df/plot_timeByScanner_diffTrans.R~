	grid.arrange(Occipital_raw_HOB_plot, Occipital_MeanSD_HOB_plot, Occipital_Mean_HOB_plot, ncol=3)
	dev.off()

	# CHR
	pdf(file = "/home/ebutler/nasa_plots/CGNCHRCGN_gmd.pdf", width=18, height=6)
	grid.arrange(AGMD_raw_CHR_plot, AGMD_MeanSD_CHR_plot, AGMD_Mean_CHR_plot, ncol=3)
	grid.arrange(BasGang_raw_CHR_plot, BasGang_MeanSD_CHR_plot, BasGang_Mean_CHR_plot, ncol=3)
	grid.arrange(Limbic_raw_CHR_plot, Limbic_MeanSD_CHR_plot, Limbic_Mean_CHR_plot, ncol=3)
	grid.arrange(FrontOrb_raw_CHR_plot, FrontOrb_MeanSD_CHR_plot, FrontOrb_Mean_CHR_plot, ncol=3)
	grid.arrange(FrontDors_raw_CHR_plot, FrontDors_MeanSD_CHR_plot, FrontDors_Mean_CHR_plot, ncol=3)
	grid.arrange(Temporal_raw_CHR_plot, Temporal_MeanSD_CHR_plot, Temporal_Mean_CHR_plot, ncol=3)
	grid.arrange(Parietal_raw_CHR_plot, Parietal_MeanSD_CHR_plot, Parietal_Mean_CHR_plot, ncol=3)
	grid.arrange(Occipital_raw_CHR_plot, Occipital_MeanSD_CHR_plot, Occipital_Mean_CHR_plot, ncol=3)
	dev.off()

} else if (type == "vol") {

	
	# ---------------------------------------------------------------------------- #
	### CGN-HOB-CGN
	# AHGMV (Average Hemisphere Total Gray Matter Volume)
	AHGMV_CGN_HOB_CGN2 <- CGN_HOB_CGN[, c("group", "subject_1", "ScanOrder", "AHGMV")] 
	AHGMVsummary <- summarySE(AHGMV_CGN_HOB_CGN2, measurevar="AHGMV", groupvars=c("group", "ScanOrder"))
	AHGMVsummary$ScanOrder <- factor(AHGMVsummary$ScanOrder)
	AHGMVsummary$group <- factor(AHGMVsummary$group)
	AHGMV_cgnhobcgn_meanSENDF <- createMeanSENDF(AHGMV_CGN_HOB_CGN2, c("T1 - CGN", "T2 - HOB", "T3 - CGN"))
	AHGMV_raw_plot <- ggplot(AHGMVsummary, aes(x=ScanOrder, y=AHGMV, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("AHGMV (+/- SE)") +
    		geom_errorbar(aes(ymin=AHGMV-se, ymax=AHGMV+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("RAW - Average Hemisphere Gray Matter Volume - CGN-HOB-CGN")
