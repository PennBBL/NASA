	# (transform)
	FrontOrb_CGN_CHR_CGN_MeanTrans <- FrontOrb_CGN_CHR_CGN2
	FrontOrb_CGN_CHR_CGN_MeanTrans$FrontOrb_Vol_Mean <- NA
	for (row in 1:nrow(FrontOrb_CGN_CHR_CGN_MeanTrans)) {
		if (FrontOrb_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "First") {
			first_mean <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "First", "FrontOrb_Vol"]
			FrontOrb_CGN_CHR_CGN_MeanTrans[row, "FrontOrb_Vol_Mean"] <- (FrontOrb_CGN_CHR_CGN_MeanTrans[row, "FrontOrb_Vol"] - first_mean)
		} else if (FrontOrb_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "Second") {
			second_mean <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "Second", "FrontOrb_Vol"]
			FrontOrb_CGN_CHR_CGN_MeanTrans[row, "FrontOrb_Vol_Mean"] <- (FrontOrb_CGN_CHR_CGN_MeanTrans[row, "FrontOrb_Vol"] - second_mean)
		} else {
			third_mean <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "Third", "FrontOrb_Vol"]
			FrontOrb_CGN_CHR_CGN_MeanTrans[row, "FrontOrb_Vol_Mean"] <- (FrontOrb_CGN_CHR_CGN_MeanTrans[row, "FrontOrb_Vol"] - third_mean)
		}
	}
	FrontOrbsummary_Mean <- summarySE(FrontOrb_CGN_CHR_CGN_MeanTrans, measurevar="FrontOrb_Vol_Mean", groupvars=c("group", "ScanOrder"))
	FrontOrb_Mean_CHR_plot <- ggplot(FrontOrbsummary_Mean, aes(x=ScanOrder, y=FrontOrb_Vol_Mean, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("FrontOrb (+/- SE)") +
    		geom_errorbar(aes(ymin=FrontOrb_Vol_Mean-se, ymax=FrontOrb_Vol_Mean+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean Trans - FrontOrb Volume - CGN-CHR-CGN")
	# FrontDors_Vol
	FrontDors_CGN_CHR_CGN2 <- CGN_CHR_CGN[, c("group", "subject_1", "ScanOrder", "FrontDors_Vol")] 
	FrontDorssummary <- summarySE(FrontDors_CGN_CHR_CGN2, measurevar="FrontDors_Vol", groupvars=c("group", "ScanOrder"))
	FrontDorssummary$ScanOrder <- factor(FrontDorssummary$ScanOrder)
	FrontDorssummary$group <- factor(FrontDorssummary$group)
	# (transform)
	FrontDors_CGN_CHR_CGN_MeanTrans <- FrontDors_CGN_CHR_CGN2
	FrontDors_CGN_CHR_CGN_MeanTrans$FrontDors_Vol_Mean <- NA
	for (row in 1:nrow(FrontDors_CGN_CHR_CGN_MeanTrans)) {
		if (FrontDors_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "First") {
			first_mean <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "First", "FrontDors_Vol"]
			FrontDors_CGN_CHR_CGN_MeanTrans[row, "FrontDors_Vol_Mean"] <- (FrontDors_CGN_CHR_CGN_MeanTrans[row, "FrontDors_Vol"] - first_mean)
		} else if (FrontDors_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "Second") {
			second_mean <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "Second", "FrontDors_Vol"]
			FrontDors_CGN_CHR_CGN_MeanTrans[row, "FrontDors_Vol_Mean"] <- (FrontDors_CGN_CHR_CGN_MeanTrans[row, "FrontDors_Vol"] - second_mean)
		} else {
			third_mean <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "Third", "FrontDors_Vol"]
			FrontDors_CGN_CHR_CGN_MeanTrans[row, "FrontDors_Vol_Mean"] <- (FrontDors_CGN_CHR_CGN_MeanTrans[row, "FrontDors_Vol"] - third_mean)
		}
	}
	FrontDorssummary_Mean <- summarySE(FrontDors_CGN_CHR_CGN_MeanTrans, measurevar="FrontDors_Vol_Mean", groupvars=c("group", "ScanOrder"))
	FrontDors_Mean_CHR_plot <- ggplot(FrontDorssummary_Mean, aes(x=ScanOrder, y=FrontDors_Vol_Mean, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("FrontDors (+/- SE)") +
    		geom_errorbar(aes(ymin=FrontDors_Vol_Mean-se, ymax=FrontDors_Vol_Mean+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean Trans - FrontDors Volume - CGN-CHR-CGN")
	# Temporal_Vol
	Temporal_CGN_CHR_CGN2 <- CGN_CHR_CGN[, c("group", "subject_1", "ScanOrder", "Temporal_Vol")] 
	Temporalsummary <- summarySE(Temporal_CGN_CHR_CGN2, measurevar="Temporal_Vol", groupvars=c("group", "ScanOrder"))
	Temporalsummary$ScanOrder <- factor(Temporalsummary$ScanOrder)
	Temporalsummary$group <- factor(Temporalsummary$group)
	# (transform)
	Temporal_CGN_CHR_CGN_MeanTrans <- Temporal_CGN_CHR_CGN2
	Temporal_CGN_CHR_CGN_MeanTrans$Temporal_Vol_Mean <- NA
	for (row in 1:nrow(Temporal_CGN_CHR_CGN_MeanTrans)) {
		if (Temporal_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "First") {
			first_mean <- Temporalsummary[Temporalsummary$group == "Phantom" & Temporalsummary$ScanOrder == "First", "Temporal_Vol"]
			Temporal_CGN_CHR_CGN_MeanTrans[row, "Temporal_Vol_Mean"] <- (Temporal_CGN_CHR_CGN_MeanTrans[row, "Temporal_Vol"] - first_mean)
		} else if (Temporal_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "Second") {
			second_mean <- Temporalsummary[Temporalsummary$group == "Phantom" & Temporalsummary$ScanOrder == "Second", "Temporal_Vol"]
			Temporal_CGN_CHR_CGN_MeanTrans[row, "Temporal_Vol_Mean"] <- (Temporal_CGN_CHR_CGN_MeanTrans[row, "Temporal_Vol"] - second_mean)
		} else {
			third_mean <- Temporalsummary[Temporalsummary$group == "Phantom" & Temporalsummary$ScanOrder == "Third", "Temporal_Vol"]
			Temporal_CGN_CHR_CGN_MeanTrans[row, "Temporal_Vol_Mean"] <- (Temporal_CGN_CHR_CGN_MeanTrans[row, "Temporal_Vol"] - third_mean)
		}
	}
	Temporalsummary_Mean <- summarySE(Temporal_CGN_CHR_CGN_MeanTrans, measurevar="Temporal_Vol_Mean", groupvars=c("group", "ScanOrder"))
	Temporal_Mean_CHR_plot <- ggplot(Temporalsummary_Mean, aes(x=ScanOrder, y=Temporal_Vol_Mean, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("Temporal (+/- SE)") +
    		geom_errorbar(aes(ymin=Temporal_Vol_Mean-se, ymax=Temporal_Vol_Mean+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean Trans - Temporal Volume - CGN-CHR-CGN")
	# Parietal_Vol
	Parietal_CGN_CHR_CGN2 <- CGN_CHR_CGN[, c("group", "subject_1", "ScanOrder", "Parietal_Vol")] 
	Parietalsummary <- summarySE(Parietal_CGN_CHR_CGN2, measurevar="Parietal_Vol", groupvars=c("group", "ScanOrder"))
	Parietalsummary$ScanOrder <- factor(Parietalsummary$ScanOrder)
	Parietalsummary$group <- factor(Parietalsummary$group)
	# (transform)
	Parietal_CGN_CHR_CGN_MeanTrans <- Parietal_CGN_CHR_CGN2
	Parietal_CGN_CHR_CGN_MeanTrans$Parietal_Vol_Mean <- NA
	for (row in 1:nrow(Parietal_CGN_CHR_CGN_MeanTrans)) {
		if (Parietal_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "First") {
			first_mean <- Parietalsummary[Parietalsummary$group == "Phantom" & Parietalsummary$ScanOrder == "First", "Parietal_Vol"]
			Parietal_CGN_CHR_CGN_MeanTrans[row, "Parietal_Vol_Mean"] <- (Parietal_CGN_CHR_CGN_MeanTrans[row, "Parietal_Vol"] - first_mean)
		} else if (Parietal_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "Second") {
			second_mean <- Parietalsummary[Parietalsummary$group == "Phantom" & Parietalsummary$ScanOrder == "Second", "Parietal_Vol"]
			Parietal_CGN_CHR_CGN_MeanTrans[row, "Parietal_Vol_Mean"] <- (Parietal_CGN_CHR_CGN_MeanTrans[row, "Parietal_Vol"] - second_mean)
		} else {
			third_mean <- Parietalsummary[Parietalsummary$group == "Phantom" & Parietalsummary$ScanOrder == "Third", "Parietal_Vol"]
			Parietal_CGN_CHR_CGN_MeanTrans[row, "Parietal_Vol_Mean"] <- (Parietal_CGN_CHR_CGN_MeanTrans[row, "Parietal_Vol"] - third_mean)
		}
	}
	Parietalsummary_Mean <- summarySE(Parietal_CGN_CHR_CGN_MeanTrans, measurevar="Parietal_Vol_Mean", groupvars=c("group", "ScanOrder"))
	Parietal_Mean_CHR_plot <- ggplot(Parietalsummary_Mean, aes(x=ScanOrder, y=Parietal_Vol_Mean, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("Parietal (+/- SE)") +
    		geom_errorbar(aes(ymin=Parietal_Vol_Mean-se, ymax=Parietal_Vol_Mean+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean Trans - Parietal Volume - CGN-CHR-CGN")
	# Occipital_Vol
	Occipital_CGN_CHR_CGN2 <- CGN_CHR_CGN[, c("group", "subject_1", "ScanOrder", "Occipital_Vol")]
	Occipitalsummary <- summarySE(Occipital_CGN_CHR_CGN2, measurevar="Occipital_Vol", groupvars=c("group", "ScanOrder"))
	Occipitalsummary$ScanOrder <- factor(Occipitalsummary$ScanOrder)
	Occipitalsummary$group <- factor(Occipitalsummary$group)
	# (transform)
	Occipital_CGN_CHR_CGN_MeanTrans <- Occipital_CGN_CHR_CGN2
	Occipital_CGN_CHR_CGN_MeanTrans$Occipital_Vol_Mean <- NA
	for (row in 1:nrow(Occipital_CGN_CHR_CGN_MeanTrans)) {
		if (Occipital_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "First") {
			first_mean <- Occipitalsummary[Occipitalsummary$group == "Phantom" & Occipitalsummary$ScanOrder == "First", "Occipital_Vol"]
			Occipital_CGN_CHR_CGN_MeanTrans[row, "Occipital_Vol_Mean"] <- (Occipital_CGN_CHR_CGN_MeanTrans[row, "Occipital_Vol"] - first_mean)
		} else if (Occipital_CGN_CHR_CGN_MeanTrans[row, "ScanOrder"] == "Second") {
			second_mean <- Occipitalsummary[Occipitalsummary$group == "Phantom" & Occipitalsummary$ScanOrder == "Second", "Occipital_Vol"]
			Occipital_CGN_CHR_CGN_MeanTrans[row, "Occipital_Vol_Mean"] <- (Occipital_CGN_CHR_CGN_MeanTrans[row, "Occipital_Vol"] - second_mean)
		} else {
			third_mean <- Occipitalsummary[Occipitalsummary$group == "Phantom" & Occipitalsummary$ScanOrder == "Third", "Occipital_Vol"]
			Occipital_CGN_CHR_CGN_MeanTrans[row, "Occipital_Vol_Mean"] <- (Occipital_CGN_CHR_CGN_MeanTrans[row, "Occipital_Vol"] - third_mean)
		}
	}
	Occipitalsummary_Mean <- summarySE(Occipital_CGN_CHR_CGN_MeanTrans, measurevar="Occipital_Vol_Mean", groupvars=c("group", "ScanOrder"))
	Occipital_Mean_CHR_plot <- ggplot(Occipitalsummary_Mean, aes(x=ScanOrder, y=Occipital_Vol_Mean, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("Occipital (+/- SE)") +
    		geom_errorbar(aes(ymin=Occipital_Vol_Mean-se, ymax=Occipital_Vol_Mean+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean Trans - Occipital Volume - CGN-CHR-CGN")

	# ---------------------------------------------------------------------------- #

	####################### MAKE PLOTS #######################

	# HOB
	pdf(file = "/home/ebutler/nasa_plots/CGNHOBCGN_vol.pdf", width=18, height=6)
	grid.arrange(AHGMV_raw_HOB_plot, AHGMV_MeanSD_HOB_plot, AHGMV_Mean_HOB_plot, ncol=3)
	#grid.arrange(AHCGMV_raw_HOB_plot, AHCGMV_MeanSD_HOB_plot, AHCGMV_Mean_HOB_plot, ncol=3)
	grid.arrange(BasGang_raw_HOB_plot, BasGang_MeanSD_HOB_plot, BasGang_Mean_HOB_plot, ncol=3)
	grid.arrange(Limbic_raw_HOB_plot, Limbic_MeanSD_HOB_plot, Limbic_Mean_HOB_plot, ncol=3)
	grid.arrange(FrontOrb_raw_HOB_plot, FrontOrb_MeanSD_HOB_plot, FrontOrb_Mean_HOB_plot, ncol=3)
	grid.arrange(FrontDors_raw_HOB_plot, FrontDors_MeanSD_HOB_plot, FrontDors_Mean_HOB_plot, ncol=3)
	grid.arrange(Temporal_raw_HOB_plot, Temporal_MeanSD_HOB_plot, Temporal_Mean_HOB_plot, ncol=3)
	grid.arrange(Parietal_raw_HOB_plot, Parietal_MeanSD_HOB_plot, Parietal_Mean_HOB_plot, ncol=3)
	grid.arrange(Occipital_raw_HOB_plot, Occipital_MeanSD_HOB_plot, Occipital_Mean_HOB_plot, ncol=3)
	dev.off()

	# CHR
	pdf(file = "/home/ebutler/nasa_plots/CGNCHRCGN_vol.pdf", width=18, height=6)
	grid.arrange(AHGMV_raw_CHR_plot, AHGMV_MeanSD_CHR_plot, AHGMV_Mean_CHR_plot, ncol=3)
	#grid.arrange(AHCGMV_raw_CHR_plot, AHCGMV_MeanSD_CHR_plot, AHCGMV_Mean_CHR_plot, ncol=3)
	grid.arrange(BasGang_raw_CHR_plot, BasGang_MeanSD_CHR_plot, BasGang_Mean_CHR_plot, ncol=3)
	grid.arrange(Limbic_raw_CHR_plot, Limbic_MeanSD_CHR_plot, Limbic_Mean_CHR_plot, ncol=3)
	grid.arrange(FrontOrb_raw_CHR_plot, FrontOrb_MeanSD_CHR_plot, FrontOrb_Mean_CHR_plot, ncol=3)
	grid.arrange(FrontDors_raw_CHR_plot, FrontDors_MeanSD_CHR_plot, FrontDors_Mean_CHR_plot, ncol=3)
	grid.arrange(Temporal_raw_CHR_plot, Temporal_MeanSD_CHR_plot, Temporal_Mean_CHR_plot, ncol=3)
	grid.arrange(Parietal_raw_CHR_plot, Parietal_MeanSD_CHR_plot, Parietal_Mean_CHR_plot, ncol=3)
	grid.arrange(Occipital_raw_CHR_plot, Occipital_MeanSD_CHR_plot, Occipital_Mean_CHR_plot, ncol=3)
	dev.off()


}








