		ggtitle("Mean SD Trans - FrontOrb Cortical Thickness - CGN-CHR-CGN")
	# FrontDors_CT
	FrontDors_CGN_CHR_CGN2 <- CGN_CHR_CGN[, c("group", "subject_1", "ScanOrder", "FrontDors_CT")] 
	FrontDorssummary <- summarySE(FrontDors_CGN_CHR_CGN2, measurevar="FrontDors_CT", groupvars=c("group", "ScanOrder"))
	FrontDorssummary$ScanOrder <- factor(FrontDorssummary$ScanOrder)
	FrontDorssummary$group <- factor(FrontDorssummary$group)
	# (transform)
	FrontDors_CGN_CHR_CGN_MeanSDTrans <- FrontDors_CGN_CHR_CGN2
	FrontDors_CGN_CHR_CGN_MeanSDTrans$FrontDors_CT_MeanSD <- NA
	for (row in 1:nrow(FrontDors_CGN_CHR_CGN_MeanSDTrans)) {
		if (FrontDors_CGN_CHR_CGN_MeanSDTrans[row, "ScanOrder"] == "First") {
			first_mean <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "First", "FrontDors_CT"]
			first_stddev <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "First", "sd"]
			FrontDors_CGN_CHR_CGN_MeanSDTrans[row, "FrontDors_CT_MeanSD"] <- (FrontDors_CGN_CHR_CGN_MeanSDTrans[row, "FrontDors_CT"] - first_mean)/first_stddev
		} else if (FrontDors_CGN_CHR_CGN_MeanSDTrans[row, "ScanOrder"] == "Second") {
			second_mean <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "Second", "FrontDors_CT"]
			second_stddev <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "Second", "sd"]
			FrontDors_CGN_CHR_CGN_MeanSDTrans[row, "FrontDors_CT_MeanSD"] <- (FrontDors_CGN_CHR_CGN_MeanSDTrans[row, "FrontDors_CT"] - second_mean)/second_stddev
		} else {
			third_mean <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "Third", "FrontDors_CT"]
			third_stddev <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "Third", "sd"]
			FrontDors_CGN_CHR_CGN_MeanSDTrans[row, "FrontDors_CT_MeanSD"] <- (FrontDors_CGN_CHR_CGN_MeanSDTrans[row, "FrontDors_CT"] - third_mean)/third_stddev
		}
	}
	FrontDorssummary_MeanSD <- summarySE(FrontDors_CGN_CHR_CGN_MeanSDTrans, measurevar="FrontDors_CT_MeanSD", groupvars=c("group", "ScanOrder"))
	FrontDors_MeanSD_CHR_plot <- ggplot(FrontDorssummary_MeanSD, aes(x=ScanOrder, y=FrontDors_CT_MeanSD, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("FrontDors (+/- SE)") +
		ylim(-1.5, 3) +
    		geom_errorbar(aes(ymin=FrontDors_CT_MeanSD-se, ymax=FrontDors_CT_MeanSD+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean SD Trans - FrontDors Cortical Thickness - CGN-CHR-CGN")
