	FrontOrb_CGN_CHR_CGN_MeanSDTrans$FrontOrb_Vol_MeanSD <- NA
	for (row in 1:nrow(FrontOrb_CGN_CHR_CGN_MeanSDTrans)) {
		if (FrontOrb_CGN_CHR_CGN_MeanSDTrans[row, "ScanOrder"] == "First") {
			first_mean <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "First", "FrontOrb_Vol"]
			first_sd <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "First", "sd"]
			FrontOrb_CGN_CHR_CGN_MeanSDTrans[row, "FrontOrb_Vol_MeanSD"] <- (FrontOrb_CGN_CHR_CGN_MeanSDTrans[row, "FrontOrb_Vol"] - first_mean)/first_sd
		} else if (FrontOrb_CGN_CHR_CGN_MeanSDTrans[row, "ScanOrder"] == "Second") {
			second_mean <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "Second", "FrontOrb_Vol"]
			second_sd <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "Second", "sd"]
			FrontOrb_CGN_CHR_CGN_MeanSDTrans[row, "FrontOrb_Vol_MeanSD"] <- (FrontOrb_CGN_CHR_CGN_MeanSDTrans[row, "FrontOrb_Vol"] - second_mean)/second_sd
		} else {
			third_mean <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "Third", "FrontOrb_Vol"]
			third_sd <- FrontOrbsummary[FrontOrbsummary$group == "Phantom" & FrontOrbsummary$ScanOrder == "Third", "sd"]
			FrontOrb_CGN_CHR_CGN_MeanSDTrans[row, "FrontOrb_Vol_MeanSD"] <- (FrontOrb_CGN_CHR_CGN_MeanSDTrans[row, "FrontOrb_Vol"] - third_mean)/third_sd
		}
	}
	FrontOrbsummary_MeanSD <- summarySE(FrontOrb_CGN_CHR_CGN_MeanSDTrans, measurevar="FrontOrb_Vol_MeanSD", groupvars=c("group", "ScanOrder"))
	FrontOrb_MeanSD_CHR_plot <- ggplot(FrontOrbsummary_MeanSD, aes(x=ScanOrder, y=FrontOrb_Vol_MeanSD, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("FrontOrb (+/- SE)") +
    		geom_errorbar(aes(ymin=FrontOrb_Vol_MeanSD-se, ymax=FrontOrb_Vol_MeanSD+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean SD Trans - FrontOrb Volume - CGN-CHR-CGN")
