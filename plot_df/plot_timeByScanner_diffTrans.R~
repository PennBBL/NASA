			second_sd <- AHCGMVsummary[AHCGMVsummary$group == "Phantom" & AHCGMVsummary$ScanOrder == "Second", "sd"]
			AHCGMV_CGN_HOB_CGN_MeanSDTrans[row, "AHCGMV_MeanSD"] <- (AHCGMV_CGN_HOB_CGN_MeanSDTrans[row, "AHCGMV"] - second_mean)/second_sd
		} else {
			third_mean <- AHCGMVsummary[AHCGMVsummary$group == "Phantom" & AHCGMVsummary$ScanOrder == "Third", "AHCGMV"]
			third_sd <- AHCGMVsummary[AHCGMVsummary$group == "Phantom" & AHCGMVsummary$ScanOrder == "Third", "sd"]
