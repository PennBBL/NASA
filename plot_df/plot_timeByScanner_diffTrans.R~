			Parietal_CGN_HOB_CGN_MeanTrans[row, "Parietal_CT_Mean"] <- (Parietal_CGN_HOB_CGN_MeanTrans[row, "Parietal_CT"] - second_mean)
		} else {
			third_mean <- Parietalsummary[Parietalsummary$group == "Phantom" & Parietalsummary$ScanOrder == "Third", "Parietal_CT"]
			Parietal_CGN_HOB_CGN_MeanTrans[row, "Parietal_CT_Mean"] <- (Parietal_CGN_HOB_CGN_MeanTrans[row, "Parietal_CT"] - third_mean)
		}
	}
	Parietalsummary_Mean <- summarySE(Parietal_CGN_HOB_CGN_MeanTrans, measurevar="Parietal_CT_Mean", groupvars=c("group", "ScanOrder"))
	Parietal_Mean_HOB_plot <- ggplot(Parietalsummary_Mean, aes(x=ScanOrder, y=Parietal_CT_Mean, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("Parietal (+/- SE)") +
		ylim(-1.5, 3) +
    		geom_errorbar(aes(ymin=Parietal_CT_Mean-se, ymax=Parietal_CT_Mean+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean Trans - Parietal Cortical Thickness - CGN-HOB-CGN")
	# Occipital_CT
