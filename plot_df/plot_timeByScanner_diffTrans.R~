	FrontDorssummary$group <- factor(FrontDorssummary$group)
	# (transform)
	FrontDors_CGN_HOB_CGN_MeanSDTrans <- FrontDors_CGN_HOB_CGN2
	FrontDors_CGN_HOB_CGN_MeanSDTrans$FrontDors_Vol_MeanSD <- NA
	for (row in 1:nrow(FrontDors_CGN_HOB_CGN_MeanSDTrans)) {
		if (FrontDors_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "First") {
			first_mean <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "First", "FrontDors_Vol"]
			first_sd <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "First", "sd"]
			FrontDors_CGN_HOB_CGN_MeanSDTrans[row, "FrontDors_Vol_MeanSD"] <- (FrontDors_CGN_HOB_CGN_MeanSDTrans[row, "FrontDors_Vol"] - first_mean)/first_sd
		} else if (FrontDors_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "Second") {
			second_mean <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "Second", "FrontDors_Vol"]
			second_sd <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "Second", "sd"]
			FrontDors_CGN_HOB_CGN_MeanSDTrans[row, "FrontDors_Vol_MeanSD"] <- (FrontDors_CGN_HOB_CGN_MeanSDTrans[row, "FrontDors_Vol"] - second_mean)/second_sd
		} else {
			third_mean <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "Third", "FrontDors_Vol"]
			third_sd <- FrontDorssummary[FrontDorssummary$group == "Phantom" & FrontDorssummary$ScanOrder == "Third", "sd"]
			FrontDors_CGN_HOB_CGN_MeanSDTrans[row, "FrontDors_Vol_MeanSD"] <- (FrontDors_CGN_HOB_CGN_MeanSDTrans[row, "FrontDors_Vol"] - third_mean)/third_sd
		}
	}
	FrontDorssummary_MeanSD <- summarySE(FrontDors_CGN_HOB_CGN_MeanSDTrans, measurevar="FrontDors_Vol_MeanSD", groupvars=c("group", "ScanOrder"))
	FrontDors_MeanSD_HOB_plot <- ggplot(FrontDorssummary_MeanSD, aes(x=ScanOrder, y=FrontDors_Vol_MeanSD, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("FrontDors (+/- SE)") +
    		geom_errorbar(aes(ymin=FrontDors_Vol_MeanSD-se, ymax=FrontDors_Vol_MeanSD+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean SD Trans - FrontDors Volume - CGN-HOB-CGN")
