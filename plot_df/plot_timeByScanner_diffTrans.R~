	# --------------------------------- MeanSD --------------------------------- #
	### CGN-HOB-CGN
	# ACT
	ACT_CGN_HOB_CGN2 <- CGN_HOB_CGN[, c("group", "subject_1", "ScanOrder", "ACT")] 
	ACTsummary <- summarySE(ACT_CGN_HOB_CGN2, measurevar="ACT", groupvars=c("group", "ScanOrder"))
	ACTsummary$ScanOrder <- factor(ACTsummary$ScanOrder)
	ACTsummary$group <- factor(ACTsummary$group)
	# (transform)
	ACT_CGN_HOB_CGN_MeanSDTrans <- ACT_CGN_HOB_CGN2
	ACT_CGN_HOB_CGN_MeanSDTrans$ACT_MeanSD <- NA
	for (row in 1:nrow(ACT_CGN_HOB_CGN_MeanSDTrans)) {
		if (ACT_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "First") {
			first_mean <- ACTsummary[ACTsummary$group == "Phantom" & ACTsummary$ScanOrder == "First", "ACT"]
			first_stddev <- ACTsummary[ACTsummary$group == "Phantom" & ACTsummary$ScanOrder == "First", "sd"]
			ACT_CGN_HOB_CGN_MeanSDTrans[row, "ACT_MeanSD"] <- (ACT_CGN_HOB_CGN_MeanSDTrans[row, "ACT"] - first_mean)/first_stddev
		} else if (ACT_CGN_HOB_CGN_MeanSDTrans[row, "ScanOrder"] == "Second") {
			second_mean <- ACTsummary[ACTsummary$group == "Phantom" & ACTsummary$ScanOrder == "Second", "ACT"]
			second_stddev <- ACTsummary[ACTsummary$group == "Phantom" & ACTsummary$ScanOrder == "Second", "sd"]
			ACT_CGN_HOB_CGN_MeanSDTrans[row, "ACT_MeanSD"] <- (ACT_CGN_HOB_CGN_MeanSDTrans[row, "ACT"] - second_mean)/second_stddev
		} else {
			third_mean <- ACTsummary[ACTsummary$group == "Phantom" & ACTsummary$ScanOrder == "Third", "ACT"]
			third_stddev <- ACTsummary[ACTsummary$group == "Phantom" & ACTsummary$ScanOrder == "Third", "sd"]
			ACT_CGN_HOB_CGN_MeanSDTrans[row, "ACT_MeanSD"] <- (ACT_CGN_HOB_CGN_MeanSDTrans[row, "ACT"] - third_mean)/third_stddev
		}
	}
	ACTsummary_MeanSD <- summarySE(ACT_CGN_HOB_CGN_MeanSDTrans, measurevar="ACT_MeanSD", groupvars=c("group", "ScanOrder"))
	ACT_MeanSD_HOB_plot <- ggplot(ACTsummary_MeanSD, aes(x=ScanOrder, y=ACT_MeanSD, colour=group, group=group)) + 
		xlab("Time Point") +
		ylab("ACT (+/- SE)") +
		ylim(-1.5, 3) +
    		geom_errorbar(aes(ymin=ACT_MeanSD-se, ymax=ACT_MeanSD+se), width=.1) +
    		geom_line() +
    		geom_point() +
		ggtitle("Mean SD Trans - Average Cortical Thickness - CGN-HOB-CGN")
